{
    "openapi": "3.0.0",
    "info": {
      "description": "DEPO swagger specification",
      "version": "1.0.0",
      "title": "swagger specification",
      "contact": {
        "email": "henry@depo.io"
      }
    },
    "servers": [
      {
        "url": "http://localhost:3000/ws/v2/",
        "description": "Backend API"
      }
    ],
    "paths": {
      "/log": {
        "get": {
          "description": "Log route, so we can check if server is alive",
          "tags": [
            "Log"
          ],
          "responses": {
            "200": {
              "description": "Log is alive",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "health": {
                        "type": "boolean"
                      },
                      "date": {
                        "type": "string"
                      }
                    },
                    "example": {
                      "health": true,
                      "date": "2018-02-19T15:36:46.758Z"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/nft/items/{contract}": {
        "get": {
          "description": "get Item base on contract",
          "tags": [
            "NFT"
          ],
          "params": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "the user identifier, as contract"
              }
            },
            "required": ["contract"]
          },
          "responses": {
            "200": {
              "description": "Items with contract",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "itemId": {
                        "type": "String"
                      } 
                    },
                    "example": {
                      "itemId": "ABCDEFG"
                    }
                  }
                }
              }
            }
          }
        }
      }


    }
  }